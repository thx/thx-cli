    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.33",
    "@types/styled-components": "^5.1.2",
    "@types/supertest": "^2.0.10",
    "@types/urijs": "^1.19.11",
    "@types/walk": "^2.3.0",
    "ali-contributors": "^2.2.2",
    "babel-eslint": "^10.1.0",
    "commitlint": "^9.1.0",
    "depcheck": "^1.2.0",
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "lerna": "^3.10.8",
    "npm-run-all": "^4.1.5",
    "shelljs": "^0.8.4",
    "typedoc": "^0.22.9",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run check",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standardx": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "globals": [
      "fetch"
    ],
    "ignore": [
      "**/*.d.ts"
    ]
  },
  "config": {
    "coffeepay": {
      "enable": false
    }
  },
